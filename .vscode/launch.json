    "version": "0.2.0",
    "configurations": [
        {
        "name": "Debugging 2D",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/mwca",
        "args": ["128", "128", "1", "10", "100"],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}/src",
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true,
                "justMyCode": false,
                "trace": true
            }
        ]
    },
    {
        "name": "GUI TEST",
        "type": "cppdbg",
        "request": "launch",
        "program": "/home/wiktor/Desktop/MW/MW_CA/test/gui/GenerartorApplication",
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}/test/gui",
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true,
                "justMyCode": false,
                "trace": true
            }
        ]
    }
]
}